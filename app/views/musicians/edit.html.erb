<div class="container mt-3">
  <h1>Edit musician</h1>
  <%= simple_form_for @musician do |f| %>
    <%= f.input :name %>
    <%= f.input :instrument %>
    <%= f.input :age %>
    <%= f.input :styles %>
    <%= f.input :location %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
  <%= link_to "Back to list", musician_path(@musician) %>
</div>
